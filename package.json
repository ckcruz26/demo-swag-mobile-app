{
  "name": "swag-app",
  "type": "module",
  "devDependencies": {
    "@types/webdriverio": "^4.13.3",
    "@wdio/allure-reporter": "^9.19.2",
    "@wdio/appium-service": "^9.19.2",
    "@wdio/cli": "^9.19.2",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "appium-uiautomator2-driver": "^5.0.0",
    "dotenv": "^17.2.1"
  },
  "scripts": {
    "test": "wdio run wdio.conf.ts",
    "allure:history": "xcopy allure-report\\history allure-results\\history /E /I /Y || echo No history yet",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "report": "npm run allure:history && npm run allure:generate && npm run allure:open"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0"
  }
}
